from tkinter import *

raiz = Tk()

raiz.title("Primera ventana Python")

raiz.resizable(1, 1)

# raiz.iconbitmap("") sirve para cambiar el icono. necesito un favicon para poner aqui!

raiz.geometry("700x400")  # cambia el tama√±o de la ventana

raiz.config(bg="green")  # cambio el color del fondo a verde

miFrame = Frame()

miFrame.pack()

miFrame.config(bg="red")

miFrame.config(width="650", heigth="350")

raiz.mainloop()
